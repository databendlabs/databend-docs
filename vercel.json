{
  "rewrites": [
    {
      "source": "/",
      "destination": "/guides/"
    }
  ],
  "headers": [
    {
      "source": "/404",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "none"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
