{
  "name": "databend-docs-v-2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "generate-release-index": "node generate-release-index.js",
    "block_before:en": "node generate-language-block.js block_before:en",
    "block_after:en": "node generate-language-block.js block_after:en",
    "block_before:cn": "node generate-language-block.js block_before:cn",
    "block_after:cn": "node generate-language-block.js block_after:cn",
    "start:cn": "site=cn site_env=development docusaurus start",
    "start:en": "site=en site_env=development docusaurus start",
    "dev": "npm run start:en",
    "dev:cn": "npm run start:cn",
    "dev:en": "npm run dev",
    "build": "npm run build:en",
    "build:cn": "npm run generate-release-index && npm run block_before:cn && site=cn site_env=production docusaurus build && npm run block_after:cn",
    "build:en": "npm run generate-release-index && npm run block_before:en && site=en site_env=production docusaurus build &&  npm run block_after:en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "parser-sitemap:en": "site=en node ./scripts/parser_sitemap.js",
    "parser-sitemap:cn": "site=cn node ./scripts/parser_sitemap.js",
    "doc-to-pdf:en": "npm run parser-sitemap:en && prince --no-warn-css --style=./src/css/print.css --input-list=./pdf/docs.databend.en-sql.txt -o ./pdf/databend-doc-en.pdf",
    "doc-to-pdf:cn": "npm run parser-sitemap:cn && prince --no-warn-css --style=./src/css/print.css --input-list=./pdf/docs.databend.cn-sql.txt -o ./pdf/databend-doc-cn.pdf",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@docusaurus/core": "3.4.0",
    "@docusaurus/plugin-client-redirects": "^3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@docusaurus/theme-mermaid": "^3.4.0",
    "@mdx-js/react": "^3.0.0",
    "ahooks": "^3.8.0",
    "antd": "^5.19.2",
    "axios": "^1.7.2",
    "clsx": "^2.0.0",
    "copyforjs": "^1.0.6",
    "databend-logos": "^0.0.16",
    "docusaurus-plugin-devserver": "^1.0.6",
    "docusaurus-plugin-sass": "^0.2.5",
    "docusaurus-prince-pdf": "^1.2.1",
    "fs-extra": "^11.2.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^9.0.1",
    "react-scroll-progress-bar": "^2.0.3",
    "sass": "^1.77.8",
    "sass-resources-loader": "^2.2.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
